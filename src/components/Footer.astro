---
import univalle from "../assets/univalleLogo.png";
---

<div class="p-4">
    <footer class="bg-neutral-primary-soft rounded-base shadow-xs border border-default m-4">
        <div class="w-full max-w-screen-xl mx-auto p-4 md:py-8">
            <div class="sm:flex sm:items-center sm:justify-between">
                <a href="https://nortedelcauca.univalle.edu.co/" class="flex items-center space-x-3 rtl:space-x-reverse">
                    <img src={univalle.src} class="h-7" alt="univalle" />
                    <span class="self-center text-xl text-heading font-semibold whitespace-nowrap">Univalle</span>
                </a>
                <ul class="flex flex-wrap items-center mb-6 text-sm font-medium text-body sm:mb-0">
                    <li>
                        <p class="hover:underline me-4 md:me-6">Tecnologias usadas en la pagina:</p>
                    </li>
                    <li>
                        <a href="https://skillicons.dev">
                            <img src="https://skillicons.dev/icons?i=astro,tailwind" />
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </footer>
</div>


