---
import Navbar from '../components/navbar.astro';
import Layout from '../layouts/Layout.astro';
import Entrega1 from "../components/Entrega1.astro";
import Portada from "../components/Portada.astro";
import Entrega2 from "../components/Entrega2.astro";
import Entrega3 from "../components/Entrega3.astro";
import Entrega4 from "../components/Entrega4.astro";
import Entrega5 from "../components/Entrega5.astro";
import Entrega6 from "../components/Entrega6.astro";
import Entrega7 from "../components/Entrega7.astro";
import Entrega8 from "../components/Entrega8.astro";
import Entrega9 from "../components/Entrega9.astro";
import Entrega10 from "../components/Entrega10.astro";
import Entrega11 from "../components/Entrega11.astro";
import Entrega12 from "../components/Entrega12.astro";
import Entrega13 from "../components/Entrega13.astro";
import Entrega14 from "../components/Entrega14.astro";
import Footer from "../components/Footer.astro";

// Navbar to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
---

<Layout>
	<Navbar />
    <Portada />
    <Entrega1 />
    <Entrega2/>
    <Entrega3/>
    <Entrega4/>
    <Entrega5/>
    <Entrega6/>
    <Entrega7/>
    <Entrega8/>
    <Entrega9/>
    <Entrega10/>
    <Entrega11/>
    <Entrega12/>
    <Entrega13/>
    <Entrega14/>
    <Footer/>
</Layout>

<script>
    if (localStorage.getItem('color-theme') === 'dark' || (!('color-theme' in localStorage) && window.matchMedia('(prefers-color-scheme: dark)').matches)) {
        document.documentElement.classList.add('dark');
    } else {
        document.documentElement.classList.remove('dark')
    }
</script>
